#!/usr/bin/env 9 rc
# /Users/korvin/apps/plan9port/bin/rc

. 9.rc
dir=acme/acme/$winid

fn rd {
	9p read $dir/$1 || { echo 9p failed >[1=2]; exit 9p }
}

tag=`{rd tag}
if(! ~ $tag(1) */){
	echo window $tag(1) is not a directory >[1=2]
	exit 'not a directory'
}

cd $tag(1)
go build -o go.out && ./go.out && rm go.out

